<apex:page standardController="ADM_Work__c" extensions="ADM_WorkControllerExtension" id="pageEdit"  title="{!recordType.Name}" cache="true">    <script type="text/javascript">                var recordType = '{!JSENCODE(recordType.Name)}';        var currRecordType = '{!JSENCODE(work.RecordType.Name)}';        var workId = '{!work.Id}';        var clone = '{!clonePageVar}';        var retUrl = '{!retUrl}';                         if(!workId){            if((/[\/a-zA-Z0-9]{15,18}/.test(retUrl)) && clone) {                workId =  '?id=' + retUrl.substring(1,retUrl.length) + '&clone=1';            }                        if(workId == '') {                workId += '?';            } else {                workId += '&';                      }            workId += 'recordTypeLogicalName=' + recordType;        } else if(workId != '') {            workId = '?id=' + workId;        } else if(workId == '') {        }                if(!retUrl) {        } else {            workId += '&retUrl=' + retUrl;        }                if(recordType != currRecordType) {            workId += '&recordTypeLogicalName=' + recordType;        }                       if(!recordType) {        } else if(recordType == 'User Story') {            top.location = '/apex/ADM_WorkEditUserStory' + workId;           } else if(recordType == 'Bug') {            top.location = '/apex/ADM_WorkEditBug' + workId;        } else if(recordType == 'ToDo') {            top.location = '/apex/ADM_WorkEditToDo' + workId;        } else if(recordType == 'Investigation') {            top.location = '/apex/ADM_WorkEditInvestigation' + workId;        } else if(recordType == 'Template') {            top.location = '/apex/ADM_WorkEditTemplate' + workId;        }            </script><!--     <apex:include pageName="ADM_WorkEdit" id="generic"/> --></apex:page>