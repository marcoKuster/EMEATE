    var view;
    var workId = ({!NOT(isNew)} ? '{!ADM_Work__c.Id}' : null);

    $(function() {
        view = new UserStoryEditView({
            app: window,
            controller: {
                getWorkAssignments: window.getWorkAssignments
            },
            productTagComponent: window.productTag,
            recordTypeId: '{!JSINHTMLENCODE(ADM_Work__c.RecordTypeId)}',
            isNew: {!isNew}
        });


