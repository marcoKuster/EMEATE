<apex:page controller="ADM_WorkLocatorController" action="{!locateWorkAndRedirect}"><!--   Copyright, 2008, SALESFORCE.com  All Rights Reserved  Company Confidential    Redirect to connect to links from external sources such as 62 Org Case object to GUS-->  <apex:stylesheet value="{!$Resource.ADM_common_css}"/> <apex:messages rendered="{!chooseWorkItem}"><div class="adm_info">{!message}</div></apex:messages><apex:messages rendered="{!NOT(chooseWorkItem)}"><div class="adm_error">{!message}</div></apex:messages><apex:pageBlock title="Located Work Records"> <apex:pageBlockTable value="{!workList}" var="work" width="100%">                       <apex:column >                 <apex:facet name="header">GUS Work Number/Name</apex:facet>                 <apex:outputLink value="/{!work.id}" target="_parent"  >                       <apex:outputText value="{!work.Name}"/>                 </apex:outputLink>                    </apex:column>             <apex:column value="{!work.Bug_Number__c}">                <apex:facet name="header">Bugforce Number</apex:facet>            </apex:column>             <apex:column value="{!work.Scrumforce_ID__c}">                <apex:facet name="header">Scrumforce ID</apex:facet>            </apex:column>            <apex:column value="{!work.Subject__c}">                <apex:facet name="header">Subject</apex:facet>            </apex:column></apex:pageBlockTable></apex:pageBlock></apex:page>