        <description>Format is: blank, NA1 (one or two digits)  NA1 NA2 or NA1 NA2 ... NA99</description>
        <errorConditionFormula>NOT(REGEX( Internal_Instances__c , &quot;|^(?:NA|CS|AP|EU|GS|SR)[0-9][0-9]?$|^(?:NA|CS|AP|EU|GS|SR)[0-9][0-9]?((?: (?:NA|CS|AP|EU|GS|SR)[0-9][0-9]?)+)* (?:NA|CS|AP|EU|GS|SR)[0-9][0-9]?&quot;))</errorConditionFormula>
        <errorDisplayField>Internal_Instances__c</errorDisplayField>
        <errorMessage>Instance list must be of the form: NA1 NA2 CS2 ...
Use capital letters and only a space between instances.
Allowed prefixes are NA, CS, EU, AP, GS or SR only.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Specify_Application_Type</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(TEXT(Application_Type__c))</errorConditionFormula>
        <errorDisplayField>Application_Type__c</errorDisplayField>
        <errorMessage>Application Type must be specified.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Specify_Category</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(TEXT( Category__c ))</errorConditionFormula>
        <errorDisplayField>Category__c</errorDisplayField>
        <errorMessage>The Application&apos;s Category must be specified.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
