    <validationRules>
        <fullName>Approval_Conext_Must_Be_Specified</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Approval_Context__c, &quot;&quot; )</errorConditionFormula>
        <errorDisplayField>Approval_Context__c</errorDisplayField>
        <errorMessage>The Approval Context must be specified.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Approval_Context_Must_Be_Specified</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Approval_Context__c, &quot;&quot; )</errorConditionFormula>
        <errorDisplayField>Approval_Context__c</errorDisplayField>
        <errorMessage>The Approval Context must be specified.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Approver_Email_must_match_users_email</fullName>
        <active>true</active>
        <description>When a Approver user is specified, if an Email is also specified it much match the system&apos;s Email for that user.</description>
        <errorConditionFormula>NOT(ISBLANK(Approver__c)) &amp;&amp; NOT(ISBLANK(Approver_Email__c )) &amp;&amp; Approver_Email__c &lt;&gt; Approver__r.Email</errorConditionFormula>
        <errorDisplayField>Approver_Email__c</errorDisplayField>
        <errorMessage>When an Approver is specified, Approver Email must be blank or match the user&apos;s system-defined email.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Approver_Role_Must_Be_Specified</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(Approver_Role__c , &quot;&quot; )</errorConditionFormula>
        <errorDisplayField>Approver_Role__c</errorDisplayField>
        <errorMessage>The Approver Role must be specified.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Approver_Type_is_Required</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( Approval_Type__c )</errorConditionFormula>
        <errorDisplayField>Approval_Type__c</errorDisplayField>
        <errorMessage>Approval Type must be specified.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Approver_must_be_specified_for_Coverage</fullName>
        <active>true</active>
        <description>An Approver must be specified for Role &quot;Release Coverage&quot;.</description>
        <errorConditionFormula>ISBLANK(Approver__c) &amp;&amp; ISPICKVAL(Approver_Role__c, &quot;Release Coverage&quot;)</errorConditionFormula>
        <errorDisplayField>Approver__c</errorDisplayField>
        <errorMessage>You must specify an Approver for Approver Role &quot;Release Coverage&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Approver_or_Email_must_be_specified</fullName>
        <active>true</active>
        <description>Either an Approver or an Email must be specified.</description>
        <errorConditionFormula>ISBLANK(Approver__c)   &amp;&amp;  ISBLANK(Approver_Email__c )</errorConditionFormula>
        <errorMessage>You must specify either an Approver or an Email.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cloud_must_be_blank_unless_Cloud_Leader</fullName>
        <active>false</active>
        <errorConditionFormula>NOT(ISPICKVAL(Approver_Role__c, &quot;Cloud Leader&quot;)) &amp;&amp;  NOT(ISBLANK(Cloud__c))</errorConditionFormula>
        <errorDisplayField>Cloud__c</errorDisplayField>
        <errorMessage>Cloud must be blank unless the Approval Role is Cloud Leader</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cloud_must_be_specified_for_Cloud_Leader</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(Approver_Role__c, &quot;Cloud Leader&quot;) &amp;&amp; ISBLANK(Cloud__c)</errorConditionFormula>
        <errorDisplayField>Cloud__c</errorDisplayField>
        <errorMessage>Cloud must be specified when Approval Role is Cloud Leader</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ExecRole_Specified_For_Executive_Approvr</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(Approver_Role__c, &quot;Executive&quot;) &amp;&amp; ISBLANK(Executive_Role__c)</errorConditionFormula>
        <errorDisplayField>Executive_Role__c</errorDisplayField>
        <errorMessage>Executive Role must be specified when &quot;Executive&quot; Approver Role is selected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Exec_Role_Blank_unless_Executive_Role</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISBLANK(Executive_Role__c)) &amp;&amp; NOT(ISPICKVAL(Approver_Role__c, &quot;Executive&quot;))</errorConditionFormula>
        <errorDisplayField>Executive_Role__c</errorDisplayField>
        <errorMessage>Approver Role must be &quot;Executive&quot; when Executive Role is specified.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Team_Blank_unless_Functional_ApproveRole</fullName>
        <active>false</active>
        <errorConditionFormula>NOT(ISBLANK(Functional_Team__c)) &amp;&amp; NOT(ISPICKVAL(Approver_Role__c, &quot;Functional&quot;))</errorConditionFormula>
        <errorDisplayField>Approver_Role__c</errorDisplayField>
        <errorMessage>Approver Role must be &quot;Functional&quot; when Functional Team is specified.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Team_Specified_Functional_Approver_Role</fullName>
        <active>true</active>
