    <validationRules>
        <fullName>Checkin_Location_Must_Be_Specified</fullName>
        <active>true</active>
        <description>When the eRelease Status is set to Approved, the default message must be replaces with checkin location and scheduled build.</description>
        <errorConditionFormula>AND( ISPICKVAL( eRelease_Status__c , &quot;Approved&quot;), OR( ISBLANK(Checkin_Location__c), 
BEGINS( Checkin_Location__c , &quot;The Release Manager will specify the checkin location for the fix and Scheduled Build to use for the checkin.&quot;)))</errorConditionFormula>
        <errorDisplayField>Checkin_Location__c</errorDisplayField>
        <errorMessage>You must remove the default text in the Checkin Details field and specify the checkin location and scheduled build.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Instances_must_be_specified</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Use_Default_Verification_Instance__c, &apos;No&apos;), ISBLANK(Instance_for_Verification__c))</errorConditionFormula>
        <errorDisplayField>Instance_for_Verification__c</errorDisplayField>
        <errorMessage>Verification instances must be specified when &quot;No&quot; selected for the &quot;Default Verification Instance?&quot; field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Release_required_when_ERR_is_Released</fullName>
        <active>true</active>
        <errorConditionFormula>( Released_In__c = NULL)   &amp;&amp;  ISPICKVAL(eRelease_Status__c, &quot;Released&quot;)</errorConditionFormula>
        <errorDisplayField>Released_In__c</errorDisplayField>
        <errorMessage>Release must be specified when eRelease Status is &quot;Released&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Specify_Target_Instances</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISPICKVAL(Deploy_to_all_Instances__c, &quot;No - specific instances&quot;), 
OR( ISBLANK(Target_Instances__c), 
BEGINS(Target_Instances__c, &quot;Specify target instances for this fix as a comma-separated list when not all instances are to receive this fix.&quot;)))</errorConditionFormula>
        <errorDisplayField>Target_Instances__c</errorDisplayField>
        <errorMessage>You must specify target instances for this fix as a comma-separated list when not all instances are to receive this fix.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_must_be_Released_when_Released_In</fullName>
        <active>true</active>
        <description>When the Released In field is used, the eRelease Status must be &apos;Released&apos; or &apos;Approved&apos;.</description>
        <errorConditionFormula>AND(
  NOT(
      OR(ISPICKVAL(eRelease_Status__c, &quot;Released&quot;),   
         ISPICKVAL(eRelease_Status__c, &quot;Approved&quot;))),  
  NOT(ISBLANK(Released_In__c))
)</errorConditionFormula>
        <errorDisplayField>eRelease_Status__c</errorDisplayField>
        <errorMessage>eRelease Status must be &apos;Released&apos; or &apos;Approved&apos; when Released In is specified. Please clear the Released In field or set the eRelease Status to &apos;Released&apos; or &apos;Approved&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Testing_Notes_required_if_Other_or_None</fullName>
        <active>true</active>
        <errorConditionFormula>AND(OR( INCLUDES(Testing_Required__c, &quot;Other&quot;), 
INCLUDES(Testing_Required__c, &quot;None required&quot;)), 
OR( ISBLANK( Other_Testing__c ), BEGINS(Other_Testing__c, &apos;Please specify testing details if &quot;None Required&quot; or &quot;Other&quot; selected.&apos;)) )</errorConditionFormula>
        <errorDisplayField>Other_Testing__c</errorDisplayField>
        <errorMessage>You must specify testing details if &quot;None Required&quot; or &quot;Other&quot; selected for Testing Required.</errorMessage>
    </validationRules>
