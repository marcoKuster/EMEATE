*{user-select:none;-webkit-user-select:none;}
body{margin:0;padding:0;background-color:#F3F6F6;font-family:'Helvetica','Arial';font-size:13px;color:#555;}
a{color:#555;}
.pageHolder{max-width:225px;overflow:hidden;}
#column1 > a{text-decoration:none;display:block;border-left:3px transparent solid;border-bottom:1px #E5E5E5 solid;padding:10px 20px 10px 10px;font-weight:bold;cursor:pointer;}
#column1 > a:hover{border-left:3px #4D90F0 solid;background:#F9F9E0;}
.icon{display:table-cell;vertical-align:middle;width:16px;height:16px;padding-right:10px;background-repeat:no-repeat;background-position-y:4px;}
.bug{background-image:url(data:image/png;base64,);}
.userstory{background-image:url(data:image/png;base64,);}
.title{display:table-cell;vertical-align:middle;}
div.fewerMore{display:none;}
span.bFilter label{display:none;}
span.bFilter span.fFooter{display:block;text-align:center;background:#ccc;padding:5px;}
span.bFilter span.fFooter a{text-decoration:none;}
span.bFilter span.fFooter a:hover{text-decoration:underline;}
span.bFilter span.fBody{display:block;text-align:center;margin-bottom:8px;}
span.bFilter select:first-child{-webkit-appearance:none;border:0;padding:10px;width:100%;background:transparent;font-size:14px;font-weight:bold;outline:0;color:#555;}
span.bFilter div.fa{margin-left:4px;font-size:16px;}
span.bFilter div.fa.deselected{color:#aaa;}
div.bNext{display:none;}    
div.pbHeader{display:none;}
#refreshButton{background-color:#fff;border-radius:10px;position:fixed;bottom:10px;right:10px;cursor:pointer;border:2px #E5E5E5 solid;padding:2px 4px;}

div#notUsedListview div.bPageBlock{display:none;}

/* listview page only: div#worklistPageHolder */
div#worklistPageHolder{}

/* backlog page only: div#backlogPageHolder */
div#backlogPageHolder span.bFilter span.fFooter{color:#ccc;}
div#backlogPageHolder span.bFilter span.fFooter a:first-child{display:none;}
div#backlogPageHolder span.bFilter span.fFooter a:first-child{margin-left:-8px;}

/* listview table manipulation */
table.list{width:100%;margin-top:15px;}
tr.headerRow{display:none;}
td.actionColumn{display:none;}
tr.dataRow th,
tr.dataRow td{display:none;font-size:13px;font-weight:normal;border-bottom:1px #E5E5E5 solid;cursor:pointer;text-align:left;}
tr.dataRow a{display:block;padding:10px 10px 10px 7px;text-decoration:none;margin-left:5px;border-top-left-radius:10px;border-bottom-left-radius:10px;}
tr.dataRow a:hover{background:#F9F9E0;}
tr.dataRow div{vertical-align:top;display:table-cell;}
tr.dataRow div.text{padding-left:8px;word-wrap:break-word;word-break:break-word;width:188px;max-width:188px;}
tr.dataRow div.fa{font-size:16px;}
tr.dataRow.selected a,
tr.dataRow.selected a:hover{background-color:#85C8DD;color:#00485F;text-shadow:0px 0px 3px #FFF;}
tr.dataRow.selected div.fa{color:#00485F;}

/* visualizer looks */
div#release-title{font-size:20px;font-weight:bold;padding:10px 10px 0px 10px;text-align:center;}
div#release-info{padding:0px 10px 10px 10px;border-bottom:1px #bee7f4 solid;margin-bottom:20px;text-align:center;}

div.sprint-complex{display:table;width:100%;}
div.sprint-complex > div{display:table-row;}
div.sprint-complex > div > div{display:table-cell;width:50%;text-align:center;padding-bottom:25px;font-weight:bold;}

.sprint{display:table;margin:0 auto;cursor:pointer;margin-bottom:3px;}
.sprint > div{display:table-row;}
.sprint > div > div{display:table-cell;border-right:2px #F3F6F6 solid;border-bottom:2px #F3F6F6 solid;height:10px;width:10px;background:#ccc;}
.sprint > div > div.filled{background:#4F97AD;}
.sprint > div > div.over{background:#AD0000;}
