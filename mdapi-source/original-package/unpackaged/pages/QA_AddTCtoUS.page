<apex:page controller="QA_AddTCtoUSController"><style type="text/css">        .TableHeader {border-bottom:solid; border-bottom-width:1px; border-bottom-color:#CCCCCC}        .TableColumn {border-right:solid; border-right-width:1px; border-right-color:#CCCCCC;}        .TableDark {background-color:#E9E9DA}</style><apex:sectionHeader title="Add Test Cases to User Story"></apex:sectionHeader><apex:pageBlock id="Error" title="Error!" mode="detail" rendered="{!init}"><apex:form id="errorAndCancel"><strong><font color="red">{!errorMessage}</font></strong><br /><apex:commandButton action="{!handleCancel}" id="Back" value="Back" /></apex:form></apex:pageBlock><apex:outputPanel id="All"><apex:form id="step1" rendered="{!ifStep1}"><apex:pageBlock id="chooseUS" title="Step 1: Select the User Story." mode="detail">        <apex:inputfield id="curUS" value="{!curTE.User_Story__c}" />        <apex:commandButton id="setUS" value="Continue" action="{!setUS}" reRender="All"/>        <apex:commandButton action="{!handleCancel}" immediate="true" id="cancel1" value="Cancel" /></apex:pageBlock></apex:form><apex:form id="step2" rendered="{!ifStep2}">        <apex:pageBlock id="detailsUS1" title="Step 1: Selected User Story." mode="detail">                <table width="100%">                        <tr>                                <td width="100"><strong>User Story:</strong></td>                                <td><a href="/{!curUS.id}">{!curUS.name}</a></td>                        </tr>                        <tr>                                <td><strong>Sprint:</strong></td>                                <td>{!curUS.Sprint__c}</td>                        </tr>                </table>        </apex:pageBlock>        <apex:pageBlock id="selectTC1" title="Step 2: Selected Test Cases." mode="detail">                <apex:outputPanel id="uniqueTCs" rendered="{!ifUniqueTCs}">                      <table width="100%" border="0" cellspacing="0" cellpadding="0">                        <tr>                          <td class="TableHeader TableDark" style="padding:3px"><strong>Unique Test Cases</strong></td>                        </tr>                        <tr>                          <td style="padding:3px">                          <apex:dataTable value="{!uniqueCases}" var="case" cellpadding="2" cellspacing="0" headerClass="TableHeader">                                <apex:column styleClass="TableColumn TableHeader">                                        <apex:facet name="header"><strong>Name</strong></apex:facet>                                        <a href="/{!case.id}">{!case.name}</a>                                </apex:column>                                <apex:column styleClass="TableColumn TableHeader">                                        <apex:facet name="header"><strong>Hierarchy</strong></apex:facet>                                        {!case.Hierarchy__c}                                </apex:column>                                <apex:column styleClass="TableColumn TableHeader">                                        <apex:facet name="header"><strong>Description</strong></apex:facet>                                        {!case.Brief_Description__c}                                </apex:column>                                <apex:column styleClass="TableHeader">                                        <apex:facet name="header"><strong>Expected Results</strong></apex:facet>                                        {!case.Expected_Results__c}                                </apex:column>                          </apex:dataTable>                                         </td>                        </tr>                        <tr>                          <td height="3"></td>                        </tr>                      </table>                </apex:outputPanel>                <apex:outputPanel id="duplicateTCs" rendered="{!ifDuplicateTCs}">                      <table width="100%" border="0" cellspacing="0" cellpadding="0">                        <tr>                          <td class="TableHeader TableDark" style="padding:3px"><strong>Duplicate Test Cases</strong> (Select&nbsp;<apex:commandLink id="selectAll" action="{!selectAll}" reRender="selectTC1">All</apex:commandLink>&nbsp;|&nbsp;<apex:commandLink id="selectNone" action="{!selectNone}" reRender="selectTC1">None</apex:commandLink>)</td>                        </tr>                        <tr>                          <td style="padding:3px">                          <apex:dataTable value="{!duplicateCases}" var="case" cellpadding="2" cellspacing="0" headerClass="TableHeader">                                <apex:column styleClass="TableColumn TableHeader">                                        <apex:facet name="header">&nbsp;</apex:facet>                                        <apex:inputCheckBox id="dupCheckbox" value="{!case.cbox}" />                                </apex:column>                                <apex:column styleClass="TableColumn TableHeader">                                        <apex:facet name="header"><strong>Name</strong></apex:facet>                                        <a href="/{!case.tc.id}">{!case.tc.name}</a>                                </apex:column>                                <apex:column styleClass="TableColumn TableHeader">                                        <apex:facet name="header"><strong>Hierarchy</strong></apex:facet>                                        {!case.tc.Hierarchy__c}                                </apex:column>                                <apex:column styleClass="TableColumn TableHeader">                                        <apex:facet name="header"><strong>Description</strong></apex:facet>                                        {!case.tc.Brief_Description__c}                                </apex:column>                                <apex:column styleClass="TableHeader">                                        <apex:facet name="header"><strong>Expected Results</strong></apex:facet>                                        {!case.tc.Expected_Results__c}                                </apex:column>                          </apex:dataTable>                                         </td>                        </tr>                      </table>                </apex:outputPanel>                <apex:commandButton id="setTCs" value="Continue" action="{!setTCs}" reRender="All" />                <apex:commandButton action="{!handleCancel}" id="cancel2" value="Cancel" />        </apex:pageBlock></apex:form><apex:form id="step3" rendered="{!ifStep3}">        <apex:pageBlock id="detailsUS2" title="Step 1: Selected User Story." mode="detail">                <table width="100%">                        <tr>                                <td width="100"><strong>User Story:</strong></td>                                <td><a href="/{!curUS.id}">{!curUS.name}</a></td>                        </tr>                        <tr>                                <td><strong>Sprint:</strong></td>                                <td>{!curUS.Sprint__c}</td>                        </tr>                </table>        </apex:pageBlock>        <apex:pageBlock id="selectTC2" title="Step 2: Selected Test Cases." mode="detail">                          <apex:dataTable value="{!allCases}" var="case" cellpadding="2" cellspacing="0" headerClass="TableHeader">                                <apex:column styleClass="TableColumn TableHeader">                                        <apex:facet name="header"><strong>Name</strong></apex:facet>                                        <a href="/{!case.id}">{!case.name}</a>                                </apex:column>                                <apex:column styleClass="TableColumn TableHeader">                                        <apex:facet name="header"><strong>Hierarchy</strong></apex:facet>                                        {!case.Hierarchy__c}                                </apex:column>                                <apex:column styleClass="TableColumn TableHeader">                                        <apex:facet name="header"><strong>Description</strong></apex:facet>                                        {!case.Brief_Description__c}                                </apex:column>                                <apex:column styleClass="TableHeader">                                        <apex:facet name="header"><strong>Expected Results</strong></apex:facet>                                        {!case.Expected_Results__c}                                </apex:column>                          </apex:dataTable>                       </apex:pageBlock>                <apex:pageBlock id="addTE" title="Step 3: Create Test Executions." mode="detail">        <apex:outputPanel id="messages">   <strong><font color="red"> <apex:messages /> </font></strong><br/> </apex:outputPanel>                        <table width="100%">                          <tr>                <td width="20%"><strong>Assignee</strong></td>                <td><apex:inputField id="assigneee" value="{!curTE.Assignee__c}"  required="true"/></td>        </tr>        <tr>                <td><strong>Status</strong></td>                <td><apex:inputField id="status" value="{!curTE.Status__c}"  required="true"/></td>        </tr>        <tr>                <td><strong>Type</strong></td>                <td><apex:inputField id="type" value="{!curTE.Type__c}"  required="true"/></td>        </tr>        <tr>                <td><strong>Date of Last Execution</strong></td>                <td><apex:inputField id="last" value="{!curTE.Date_of_Last_Execution__c}" required="{!dateRequired}"/></td>        </tr>        <tr>                <td><strong>Number of Executions</strong></td>                <td><apex:inputField id="num" value="{!curTE.Number_of_Executions__c}" /></td>        </tr>        <tr>                <td><strong>Build # of Last Execution</strong></td>                <td><apex:inputField id="build" value="{!curTE.Build_of_Last_Execution__c}" /></td>        </tr>        <tr>                <td><strong>Bugs</strong></td>                <td><apex:inputField id="bugs" value="{!curTE.Bugs__c}" /></td>        </tr>                                             </table>                <apex:commandButton id="save" value="Create Executions" action="{!handleSave}" rerender="messages"/>                <apex:commandButton action="{!handleCancel}" id="cancel3" value="Cancel" immediate="true"/>        </apex:pageBlock></apex:form></apex:outputPanel></apex:page>